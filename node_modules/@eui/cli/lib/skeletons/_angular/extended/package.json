{
    "name": "angular-app-root",
    "version": "0.1.0",
    "license": "EUPL-1.1",
    "scripts": {
        "clean:deps": "rimraf node_modules",
        "create:symlinks": "node \"./tools/scripts/init.js\"",
        "postinstall": "npm run create:symlinks",
        "app:build": "cd src/app && npm run build",
        "app:build-prod": "cd src/app && npm run build-prod",
        "pkg:clean": "eui-scripts clean-package",
        "pkg:clean-all": "eui-scripts clean-all",
        "pkg:build": "eui-scripts build-package",
        "pkg:build-deps": "eui-scripts build-deps",
        "pkg:build-all": "eui-scripts build-all",
        "build": "npm run pkg:build-all && npm run app:build",
        "build-prod": "npm run pkg:build-all && npm run app:build-prod",
        "publish": "eui-scripts publish",
        "publish-all": "eui-scripts publish-all",
        "publish-all:ecdevops": "npm run publish-all -- --ecdevops",
        "publish-all:verdaccio": "npm run publish-all -- --verdaccio",
        "start": "npm run pkg:clean-all && cd src/app && npm run start",
        "start-proxy": "npm run pkg:clean-all && cd src/app && npm run start-proxy"
    },
    "private": true,
    "dependencies": {
        "@eui/deps-base": "3.6.11"
    },
    "devDependencies": {
        "@angular/cli": "1.7.4",
        "karma": "1.7.1",
        "typescript": "2.4.2"
    }
}